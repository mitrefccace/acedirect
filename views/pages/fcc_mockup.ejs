            selected = 'selected';
          }
          options += '<option value="';
          options += option.value;
          options += '" ' + selected + '>';
          if (option.label == '-') {
            options += 'Select an Option';
          } else {
            options += option.label;
          }
          options += '</option>';
        });
      }
      options += '</select></div>';
      $ae(this).before(options);
    });


    // Connect select fields to dynamic form changes
    ae.find('.ae-select').change(function(e) {
      //e.preventDefault();
      //e.stopPropagation();

      var input_id = $ae(this).attr('data-id');
      var value = $ae(this).val();
      ae.find('#' + input_id).val(value);
      $('#' + input_id).change();

      var option_text = $ae(this).find('option:selected').text();
      $ae(this).parent('.ae-offscreen').next('.nesty-input').text(option_text);
      $ae(this).focus();

    });

    // Make input type of text
    //ae.find('input#query').attr('type','text');

    // Make notices assertive
    ae.find('.notification-notice').attr('aria-live', 'assertive').attr('role', 'alert');
    ae.find('.notification-error').attr('aria-live', 'assertive').attr('role', 'alert');

    // Combine footer menu items into one system menu
    ae.find('footer').append('<ul id="ae-footer-menu" class="ae-hide" style="display:none;"></ul>');
    ae.find('footer ul li a').each(function() {
      $ae(this).parent().clone().appendTo('#ae-footer-menu');
    });

    // example of changing voice on RADAE audio
    ae.find("#ae-app").on("StateChange", function(e) {
      switch (e.originalEvent.detail.state) {
        case window.aeplayer.playerStates.RADAE_FINISH:
          if (ae.find('h4:contains("Para presentar")').length > 0) {
            var audioasset = ae.find('h4:contains("Para presentar")').attr("data-ae-audio");
            audioasset += '&voice=usspanishfemale';
            //console.log(audioasset);
            //ae.find('h4:contains("Para presentar")').attr('data-ae-audio',audioasset);

          }
          break;
      }
    });
    ae.find('p:contains("Para presentar")').attr('lang', 'es');

    // Fix issue with second fake datepicker field by making it hidden in addition to display none
    ae.find('.datepicker').next('input').attr('type', 'hidden');


    // Adding "hidden" spans to label for help text
    ae.find('.request-form input:not([type=hidden]),.request-form textarea').each(function() {
      if ($ae(this).hasClass('datepicker')) {
        var p = $ae(this).siblings('p').text();
        p = p.replace(/-/g, ' dash ').replace(/:/g, ' colon ');
        $ae(this).siblings('label').append('<span style="position: absolute; opacity: 0;" class="ae-label-span-desc"> ' + p + '</span>');
      } else {
        var p = $ae(this).siblings('p').text();
        p = p.replace(/-/g, ' dash ').replace(/:/g, ' colon ');
        $ae(this).siblings('label').append('<span style="position: absolute; opacity: 0;" class="ae-label-span-desc"> ' + p + '</span>');
      }
    });

    // Clarify Common Issues/File Complaint anchors with aria-label
    ae.find('a:contains(Common Issues)').each(function() {
      $ae(this).attr('aria-label', 'Common ' + $ae(this).closest('.group').prev('h3').text() + ' Issues');
    });

    ae.find('a:contains(File Complaint)').each(function() {
      $ae(this).attr('aria-label', 'File ' + $ae(this).closest('.group').prev('h3').text() + ' Complaint');
    });

    ae.find('a[href="https://p2.zdassets.com/hc/theme_assets/513073/200051444/12-2Informal_Complaint_FormAccessible.docx"]').attr('aria-label', 'Download Word Complaint Form');

    ae.find('a[href="https://p2.zdassets.com/hc/theme_assets/513073/200051444/Informal-Complaint-Form-Accessible.docx"]').attr('aria-label', 'Download Word Complaint Form');

    ae.find('a[href="https://p2.zdassets.com/hc/theme_assets/513073/200051444/12-2_Informal_Complaint_FormAccessible.pdf"]').attr('aria-label', 'Download PDF Complaint Form');

    // Make breadcrumbs list read like a div
    ae.find('ol.breadcrumbs').attr('data-ae-blurbtype', 'div');



    // Use Mutation Observer to detect Recaptcha changes and fix replay audio link with an href when it is reloaded
    // The node to be monitored
    if (ae.find("#recaptcha_image").length > 0) {
      var target = ae.find("#recaptcha_image")[0];
      // Create an observer instance
      var observer = new MutationObserver(function(mutations) {
        mutations.forEach(function(mutation) {
          var newNodes = mutation.addedNodes; // DOM NodeList
          if (newNodes !== null) { // If there are new nodes added
            var $nodes = ae.find(newNodes); // jQuery set
            $nodes.each(function() {
              var $node = ae.find(this);
              // Find audio replay link and give it an href so it will be picked up as a tab and the JS function so hitting enter will fire the replay audio function
              $node.find('#recaptcha_audio_play_again').attr('href', 'javascript:Recaptcha.playAgain()');



              // Refresh form blurb
              var formblurb = window.aeplayer.blurbFactory(ae.find('#registration-form'));
              formblurb.digest = 0;
              formblurb.refresh();
            });
          }
        });
      });
      // Configuration of the observer:
      var config = {
        attributes: true,
        childList: true,
        characterData: true
      };
      // Pass in the target node, as well as the observer options
      observer.observe(target, config);
    }


    // Focus on response field input when clicking on play audio again link
    ae.find('#recaptcha_audio_play_again').click(function() {
      ae.find('#recaptcha_response_field').focus();
      //window.aeplayer.goToLandmark('#recaptcha_response_field');
    });

    // Send player focus to audio text input field on click of switch to audio captcha
    ae.find('#recaptcha_switch_audio_btn').click(function() {
      //window.aeplayer.goToLandmark('#recaptcha_response_field');
    });

    // Treat UL in recaptcha as a standard DIV
    ae.find('#recaptcha_widget ul').attr('data-ae-blurbtype', 'div');



    // Change inputs tab indexes to match the error messages
    if (ae.find('.notification-error .notification-text ul li:first').length > 0) {
      ae.find('input,textarea,select').attr('tabindex', '1');
      ae.find('.notification-dismiss').attr('tabindex', '1').attr('aria-label', 'Press tab to review the form , , , ,');
    }

    ae.find('.notification-error .notification-text ul li').each(function() {
      var arr = $ae(this).text().split(':');
      ae.find('label:contains("' + arr[0] + '")').next('input,textarea,select').attr('tabindex', '1');
    });


    // Add required span to labels that are required
    ae.find('.required label').each(function() {
      $ae(this).append('<span style="position: absolute; opacity: 0;" class="ae-required-label"> REQUIRED </span>');
    });


    // Identify Add Files button properly
    ae.find('#request-attachments').attr('placeholder', 'Attachments, browse to attach a file');

    var inputs = ae.find('select:not([disabled]):not(:hidden),input:not([disabled]):not([type="hidden"]):not(:hidden),textarea:not([disabled]):not(:hidden)');
    var tabInput;
    var currentInput;

    ae.find('div.form').focus(function() {
      //console.log(currentInput);
      if (currentInput != null && currentInput.length > 0) {

        if (currentInput.is(tabInput)) {

          console.log('form focus - matching inputs');
          console.log(tabInput[0].id);
          console.log(currentInput[0].id);
          inputs = ae.find('select:not([disabled]):not(:hidden),input:not([disabled]):not([type="hidden"]):not(:hidden),textarea:not([disabled]):not(:hidden)');

          var index = inputs.index(ae.find('#' + currentInput[0].id));
          console.log(inputs);
          console.log(index);
          ++index;
          console.log(index);
          inputs[index].focus();

        } else {
          currentInput.focus();
        }
      }
    });

    ae.find('select,input,textarea').focus(function() {
      currentInput = $ae(this);
      console.log('input focus');
      console.log(currentInput[0].id);
    });

    // Give submit button a way to notify users that it is disabled and not present because required fields are missing
    ae.find('input[type="submit"]').before('<div tabindex="0" class="missing-required-fields" style="display:none; position:absolute; opacity:0;">Required Fields are missing. Please fill out all required fields to submit.</div>');

    // Fire events on press of tab key
    ae.find('select,input,textarea').keydown(function(e) {
      if (e.keyCode == 9 && !e.shiftKey) {

        // Set input that was selected when tab was pressed
        tabInput = $ae(this);
        console.log('Press Key Button');
        console.log(tabInput[0].id);

        // Redo Required labels on tab since required classes changes often
        ae.find('.ae-required-label').remove();
        ae.find('.required label').each(function() {
          $ae(this).append('<span style="position: absolute; opacity: 0;" class="ae-required-label"> REQUIRED </span>');
        });

        // Toggle disabled submit button message due to missing required fields
        if (ae.find('input[type="submit"]:disabled').length > 0) {
          ae.find('.missing-required-fields').show();
        } else {
          ae.find('.missing-required-fields').hide();
        }


      }
    });

    // Fix datepicker label
    ae.find('.datepicker').each(function() {
      var label = $ae(this).prev('label');
      label.attr('for', label.attr('for') + 'date');
      $ae(this).attr('id', label.attr('for') + 'date');
    });


    // Give pagination elements useful aria labels
    ae.find('.pagination-first a').attr('aria-label', 'First Page');
    ae.find('.pagination-prev a').attr('aria-label', 'Previous Page');
    ae.find('.pagination-next a').attr('aria-label', 'Next Page');
    ae.find('.pagination-last a').attr('aria-label', 'Last Page');
    ae.find('.pagination-current span').attr('aria-label', 'Current Page');

    ae.find('.pagination-current span').each(function() {
      $ae(this).attr('aria-label', 'Current Page ' + $ae(this).text());
    });

    ae.find('nav.pagination li:not([class]) a').each(function() {
      $ae(this).attr('aria-label', 'Page ' + $ae(this).text());
    });


    // Fix focus problem on IE so that cursor starts at the end of the input
    var ms_ie = false;
    var ua = window.navigator.userAgent;
    ms_ie = ~ua.indexOf('MSIE ') || ~ua.indexOf('Trident/');
    if (ms_ie <= -1) {
      ms_ie = true;
    }
    if (ms_ie) {
      ae.find('input[type!="submit"],textarea').attr('onfocus', 'this.value = this.value;');
    }

    // Make this error/notification UL act like a DIV
    ae.find('.notification-text ul').attr('data-ae-blurbtype', 'div');

    // Pete prefers to hear this as a link even though they have defined the link to act like a button. Removing role attr makes it be read as a link again
    ae.find('.login').removeAttr('role');

    // Add proper text the link to close the notification of form submission
    ae.find('.notification-dismiss').html('<span class="ae-compliance-indent" style="display:none;">dismiss notification</span>');

    // Add hidden offscreen text before and after Spanish language text based on the lang="es" attribute
    ae.find('[lang="es"]:not(html)').before('<span class="ae-compliance-indent" style="display:none;">Spanish Begin</span>').after('<span class="ae-compliance-indent" style="display:none;">Spanish End</span>');

    //rename form in forms list
	ae.find('form#new_request').attr('aria-label', 'Complaint Feedback Form');
    ae.find('form.search').attr('aria-label', 'Site Search Form');
    
    //append text to vague link
    ae.find('a[href="https://p2.zdassets.com/hc/theme_assets/513073/200051444/12-2_Informal_Complaint_FormAccessible.pdf"]').append('<span class="ae-compliance-indent" style="display:none;"> Downloadable Complaint Form </span>');
    ae.find('a[href="/hc/en-us/articles/204009760-Consumer-Help-Center-Topline-Charts-and-Data"]').append('<span class="ae-compliance-indent" style="display:none;"> for overview charts and data for the Help Center </span>');
    ae.find('a[href="/hc/en-us/articles/204537720"]').append('<span class="ae-compliance-indent" style="display:none;"> for in depth charts and data by complaint category </span>');
    ae.find('a[href="/hc/en-us/articles/204646900"]').append('<span class="ae-compliance-indent" style="display:none;"> for charts and data by service type </span>');

    ae.find('.notification-notice').attr('tabindex', '0');
    ae.find('.notification-notice').focus();
    //window.focus();
    
    // Adjust tab order in header so it matches visual order
    ae.find('header.header a').attr('tabindex','1');
    setTimeout(function(){
      ae.find('.ae-skipto-main').attr('tabindex','1');
    }, 500);

  });
  
  // Set focus to main area on initial load of search page to prevent focus happening on search input below search results
  if(window.location.pathname == "/hc/en-us/search") {
    AudioEye.focusElement('main');
  }

